{
	"singleQuote": true,
	"useTabs": true,
	"trailingComma": "es5",
	"arrowParens": "always",
	"importOrder": [
		"^react",
		"^@?\\w",
		"^(@|components)(/.*|$)",
		"^\\u0000",
		"^\\.\\.(?!/?$)",
		"^\\.\\./?$",
		"^\\./(?=.*/)(?!/?$)",
		"^\\.(?!/?$)",
		"^\\./?$",
		"^.+\\.?(css)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
